{
  "import statement": {
    "prefix": "im",
    "body": ["import $1 from '$2'"]
  },
  "import type statement": {
    "prefix": "imt",
    "body": ["import type $1 from '$2'"]
  },
  "export as": {
    "prefix": "exa",
    "body": ["export * as $1 from '$2'"]
  },
  "export from": {
    "prefix": "exf",
    "body": ["export { $1 } from '$2$1 from '$1'"]
  },
  "export async function": {
    "prefix": "eaf",
    "body": ["export async function $1(): $2 {\n$3\n}"]
  },
  "export type": {
    "prefix": "et",
    "body": ["export type $1 = {\n  $2\n}"]
  },
  "import cc-specific packages": {
    "prefix": "cc",
    "body": ["import $1 from '@connectedcars/$2'"]
  }
}
